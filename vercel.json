{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.kotourameishomaru.com"
        }
      ],
      "destination": "https://kotourameishomaru.com/:path*",
      "permanent": true
    }
  ]
}